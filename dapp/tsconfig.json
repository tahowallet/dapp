{
  "compilerOptions": {
    "target": "esnext",
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noImplicitOverride": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["shared/components/*"],
      "@assets/*": ["shared/assets/*"],
      "@shared/*": ["shared/*"]
    }
  }
}
